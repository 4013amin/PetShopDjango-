{
  "app": "petshopdjango",
  "port": 2020, 
  "build": {
    "location": "iran"
  },
  "command": "daphne -b 0.0.0.0 -p 2020 ShopApp.asgi:application", 
  "disks": [
    {
      "name": "media",
      "mountTo": "/media"
    },
    {
      "name": "data",
      "mountTo": "/data"
    }
  ],
  "environment": {
    "DJANGO_DEBUG": "False",
    "REDIS_HOST": "redis-1234.liara.run",
    "REDIS_PORT": "6379"
  }
}