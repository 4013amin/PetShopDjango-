{
  "app": "petshopdjango",
  "port": 8000,
  "build": {
    "location": "iran"
  },
  "command": "daphne -b 0.0.0.0 -p 8000 ShopApp.asgi:application",
  "disks": [
    {
      "name": "media",
      "mountTo": "/usr/src/media"
    },
    {
      "name": "data",
      "mountTo": "/usr/src/data"
    }
  ]
}